{
    "version": 2,
    "builds": [
      {
        "src": "api/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/swagger.json",
        "dest": "api/public/swagger.json"
      },
      {
        "src": "/(.*)",
        "dest": "api/app.js"
      },
      {
        "src": "/api-docs",
        "dest": "api/app.js"
      }
    ]
  }