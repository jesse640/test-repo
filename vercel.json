{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "api/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/app.js"
    },
    {
      "src": "/swagger.yaml",
      "dest": "/swagger.yaml"
    }
  ]
}
