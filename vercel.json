{
  "version": 2,
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "api/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/app.js"
    },
    {
      "src": "/api/public/swagger.yaml",
      "dest": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/4.15.5/swagger-ui.min.css"
    }
  ]
}
